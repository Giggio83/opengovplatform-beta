	<script type="text/javascript">
      google.load('visualization', '1', {packages: ['corechart', 'table', 'geomap', 'motionchart']});
      google.setOnLoadCallback(DataViz.drawVisualization);
	  
      function initialize() {
        var mapDiv = document.getElementById('data_viz_map');
        var map = new google.maps.Map(mapDiv, {
          center: new google.maps.LatLng(37.4419, -122.1419),
          zoom: 13,
          mapTypeId: google.maps.MapTypeId.ROADMAP
        });
      
      }
      google.maps.event.addDomListener(window, 'load', initialize);
    </script>
    <script type="text/javascript" src="js/script.js"></script>
</head>
<body>
	<div class="container-fluid">
		<div class="row-fluid">
			<div class="span12">
				<div class="page-header">
			    	<h1>Data.gov.in Visualization Engine <small>come, explore, share ... </small></h1>
			    </div>
			</div>
		</div>
		<div class="row-fluid information">
      <div class="span12">
      <div class="graphicon-circle-info pull-right" onclick="DataViz.toggleDatasetInfo();"></div>
      <div class="clearfix"></div>
        <div class="dataset-details alert alert-info" style="display:none;">
            <h2 id="dataset-title"></h2><small><span id="dataset-email"></span></small>
            <p id="dataset-desc"></p>
          </div>
      </div>
    </div>
		<div class="row-fluid">
			<div class="span12">
				<!--Sidebar content-->
				<!-- 
		    	<form class="navbar-search pull-right">
			    	<input type="text" class="search-query" placeholder="Search">
			    </form>
				-->
				<div id="data_component" class="btn-group pull-right" data-toggle="buttons-checkbox">
				    <button type="button" title="Grid" class="btn grid active">
						<span class="graphicon-grid"></span>
					</button>
				    <button type="button" title="Chart" class="btn chart">
						<span class="graphicon-chart"></span>
					</button>
				  <button type="button" title="Map" class="btn map">
						<span class="graphicon-map"></span>
					</button>
					<button type="button" title="NIC Map" class="btn arcgismap">
            <span class="graphicon-map1"></span>
          </button>
		    	</div>
		    	<div class="btn-group">
            <a class="btn btn-success dropdown-toggle" data-toggle="dropdown" href="#">
            Choose Data Source
            <span class="caret"></span>
            </a>
           <ul class="dropdown-menu">
             <!-- dropdown menu links -->
             <li><a href="datawizard.php">CSV Data Source</a></li>
             <li><a href="#ModalgoogleSpreadsheet" data-toggle="modal">Google Spread Sheet</a></li>
             <li><a href="#ModalDataGovIn" data-toggle="modal">Data.gov.in Data Source</a></li>
           </ul>
         </div>
        <div id="ModalgoogleSpreadsheet" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
              <h3 id="myModalLabel">Google Spread Sheet DataSource</h3>
            </div>
            <div class="modal-body">
            <form>
            <label>Provde Google Spreadsheet Link</label><input type="text" placeholder="google spreadsheet Link" name="spreadsheet_source" id="google-spreadsheet-source" />
            </form>
            </div>
            <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
            <button class="btn btn-primary" onclick="DataViz.setDatasource('google_spreadsheet');">Visualize</button>
        </div>
        </div>
         <div id="ModalDataGovIn" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="ModalDataGovInLabel" aria-hidden="true">
            <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
              <h3 id="ModalDataGovInLabel">Data.gov.in Dataset Source</h3>
            </div>
            <div class="modal-body">
            <form>
            <label>Provide Data.gov.in Dataset Instance</label><input type="text" placeholder="Data.gov.in Instance" name="datagovin_source" id="datagovin-source" />
            </form>
            </div>
            <div class="modal-footer">
            <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
            <button class="btn btn-primary datasource" onclick="DataViz.setDatasource('data_gov_in_source');">Visualize</button>
        </div>
        </div>
			    <div class="clearfix"></div>
				<div class="record-count"></div>
			</div>
